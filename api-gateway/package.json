{
  "name": "giga-api-gateway",
  "version": "1.0.0",
  "description": "Unified API Gateway for Giga Platform - Routes requests between Supabase and Railway services",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:property": "jest --testPathPattern=property.test.js",
    "test:all": "npm test && npm run test:property",
    "build": "echo 'No build step required for Node.js'",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "http-proxy-middleware": "^2.0.6",
    "@supabase/supabase-js": "^2.38.4",
    "jsonwebtoken": "^9.0.2",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1",
    "node-cache": "^5.1.2",
    "axios": "^1.6.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.55.0",
    "fast-check": "^3.15.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "api-gateway",
    "microservices",
    "supabase",
    "railway",
    "routing"
  ],
  "author": "Giga Platform Team",
  "license": "MIT"
}
